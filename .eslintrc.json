{
  "extends": ["lffg"],
  "settings": {
    "react": {
      "version": "latest"
    }
  },
  "rules": {
    "no-restricted-globals": "off",
    "no-unused-vars": "off"
  },
  "globals": {
    "jQuery": true
  },
  "overrides": [
    {
      "files": "./fdf/*",
      "rules": {
        "strict": ["error", "global"],
        "prettier/prettier": ["error", { "semi": true }]
      }
    }
  ]
}
